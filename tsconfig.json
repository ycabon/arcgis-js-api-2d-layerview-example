{
  "compilerOptions": {
		"declaration": false,
    "experimentalDecorators": true,
    "noEmitHelpers": true,
    "strictNullChecks": false,
		"module": "umd",
		"noUnusedLocals": true,
		"removeComments": true,
		"sourceMap": true,
    "target": "es5",
    "outDir": "dist"
  },
  "include": [
    "./src/**/*.ts",
    "./typings/**/*.d.ts"
  ]
}